{
  "name": "pickmoji",
  "version": "0.0.1",
  "description": "Google Chrome / Chromium extension for emoji auto-completion",
  "scripts": {
    "build": "node ./src/extension/generateEmojis.js && webpack --config webpack/webpack.config.prod.js  --colors",
    "start": "node ./src/extension/generateEmojis.js && webpack-dev-server --open --config webpack/webpack.config.dev.js"
  },
  "author": "vgaviria",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.4.4",
    "@babel/preset-env": "^7.4.2",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^2.0.2",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint-config-google": "^0.13.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.12.0",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.0",
    "emoji.json": "^12.1.0",
    "eslint": "^5.16.0",
    "textarea-caret": "^3.1.0"
  }
}
